{
  "name": "book",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "build/index.html"
  },
   "permissions": [
    "cookies",
    "alarms",
    "contextMenus",
    "storage",
    "notifications",
    "bookmarks",
    "<all_urls>"
  ]
}